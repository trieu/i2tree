{
  "name": "eClickAdsSimulator",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "eClickAdsSimulator - The Simulator Tools for FPT eClick",   
  "permissions": [
    "background" , "tabs", "unlimitedStorage", "http://*/*"
  ],
  "icons": {"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery.min.js","utils.js"]
    }
  ]
}